<?xml version="1.0" encoding="UTF-8" ?>
<ResourceDictionary
    x:Class="TabbedPage.Maui.Themes.Views.LukeTabbedPage"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:d="http://schemas.microsoft.com/dotnet/2021/maui/design"
    xmlns:local="clr-namespace:TabbedPage.Maui.Themes.Views"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:views="clr-namespace:TabbedPage.Maui.UI.Views"
    mc:Ignorable="d">
    <Style TargetType="{x:Type views:LukeTabbedPage}">
        <Setter Property="ControlTemplate">
            <ControlTemplate>
                <Grid>
                    <ContentPresenter Content="{Binding CurrentPage}" />
                    <ContentView Content="{Binding Navigator}" />
                </Grid>
            </ControlTemplate>
        </Setter>
    </Style>
</ResourceDictionary>
